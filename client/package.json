{
    "name": "swengineer-client",
    "version": "2.2.0",
    "author": "Toby Stayner <toby@swengineer.dev>",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource/cascadia-mono": "^4.2.1",
        "@fontsource/roboto": "^5.2.8",
        "@mui/icons-material": "^6.5.0",
        "@mui/lab": "6.0.0-beta.26",
        "@mui/material": "^6.5.0",
        "i18next": "^21.10.0",
        "i18next-browser-languagedetector": "^8.2.0",
        "notistack": "3.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^11.18.6",
        "react-router-dom": "^7.9.1"
    },
    "devDependencies": {
        "@types/i18next": "^13.0.0",
        "@types/node": "^22.18.6",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "eslint": "^8.57.1",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react": "^7.37.5",
        "react-scripts": "5.0.1",
        "sass": "^1.92.1",
        "serve": "^14.2.5",
        "sitemap": "^8.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5"
    },
    "main": "index.js",
    "scripts": {
        "generate-sitemap": "ts-node --compilerOptions '{\"module\": \"CommonJS\"}' generate-sitemap.ts",
        "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build && pnpm generate-sitemap",
        "clean": "rm -rf build dist node_modules",
        "dev": "PORT=3000 react-scripts start",
        "start": "serve -s build",
        "test": "react-scripts test --passWithNoTests --watchAll=false",
        "coverage": "nyc --reporter=html --reporter=text-summary pnpm test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:8080"
}
