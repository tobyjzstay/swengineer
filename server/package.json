        "passport-google-oauth20": "^2.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.28.4",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/express-session": "^1.18.2",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.18.6",
