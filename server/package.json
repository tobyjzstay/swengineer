    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf .nyc_output coverage dist node_modules",
        "dev": "NODE_ENV=development nodemon --watch \"src/**\" --ext \"ts, json\" --exec \"ts-node --transpileOnly src/index.ts\"",
        "start": "NODE_ENV=production node dist/index.js",
        "test": "NODE_ENV=test mocha --require ts-node/register test/**/*.ts --recursive --exit --timeout 10000",
        "coverage": "nyc --reporter text --reporter html pnpm test"
